<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <LangVersion>latest</LangVersion>
    <PackageProjectUrl>https://github.com/astef/SparseNumerics</PackageProjectUrl>
    <PackageLicenseUrl>https://github.com/astef/SparseNumerics/blob/master/LICENSE</PackageLicenseUrl>
    <Authors>Vladimir Reshetnikov, Alexander Stefurishin</Authors>
    <Description>An integer stored as the positions of 1's in its binary form, useful for math with high power towers.</Description>
    <RepositoryUrl>https://github.com/astef/SparseNumerics</RepositoryUrl>
    <PackageTags>Math, BigInteger, binary</PackageTags>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <Version>0.0.0-alpha</Version>
  </PropertyGroup>

</Project>
